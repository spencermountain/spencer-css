greys = {
  offwhite : #fbfbfb,
  light: #a3a5a5,
  dim  : #d7d5d2,
  grey     : #4d4d4d,
  mid  : #949a9e,
  slate: #51433e,
  blue : #606c74,
  dark : #443d3d,
  offblack : #333333
}

for key, val in colors
  //text colors
  .{key}
    color:val
  //backgrounds
  .bg{key}
    background-color:val
  //underlines
  .ul{key}
    border-bottom:2px solid val
    padding-left:1rem
    padding-right:1rem



